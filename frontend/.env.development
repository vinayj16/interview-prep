# ====================================
# Development Environment Configuration
# ====================================

# Application
REACT_APP_NAME="Interview Preparation Platform (Development)"
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development
REACT_APP_DEFAULT_LANGUAGE=en
REACT_APP_DEBUG=true

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:5000
REACT_APP_API_VERSION=v1
REACT_APP_API_TIMEOUT=30000

# Authentication
REACT_APP_AUTH_TOKEN_KEY=interview_prep_auth_token_dev
REACT_APP_REFRESH_TOKEN_KEY=interview_prep_refresh_token_dev
REACT_APP_TOKEN_EXPIRY_CHECK=300000  # 5 minutes in ms

# Gemini API (for AI features)
REACT_APP_GEMINI_API_KEY=your_gemini_api_key_here

# Feature Flags
REACT_APP_FEATURE_ANALYTICS=false
REACT_APP_FEATURE_OFFLINE_MODE=true
REACT_APP_FEATURE_DARK_MODE=true

# Performance
REACT_APP_PERF_MEASURE=true
REACT_APP_PERF_LONG_TASK_THRESHOLD=200
